{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated",
    "FUNCTIONS_EXTENSION_VERSION": "~4",
    "APPLICATIONINSIGHTS_CONNECTION_STRING": "InstrumentationKey=your-instrumentation-key;IngestionEndpoint=https://your-region.in.applicationinsights.azure.com/;LiveEndpoint=https://your-region.livediagnostics.monitor.azure.com/;ApplicationId=your-application-id",
    "STORAGE_ACCOUNT_NAME": "your-storage-account-name",
    "FILE_SHARE_NAME": "valheim-worlds",
    "CONTAINER_GROUP_NAME": "valheim-server",
    "RESOURCE_GROUP_NAME": "valheim-server-rg",
    "AUTO_SHUTDOWN_MINUTES": "120",
    "SERVER_NAME": "My Valheim Server",
    "LOCATION": "eastus",
    "SUBSCRIPTION_ID": "your-subscription-id",
    "DISCORD_PUBLIC_KEY": "your-discord-public-key-here",
    "SERVER_PASSWORD": "your-server-password-here",
    "ACR_LOGIN_SERVER": "valheimacrXXXX.azurecr.io (unique name - check deployment outputs)",
    "ACR_USERNAME": "valheimacrXXXX (ACR name without .azurecr.io)",
    "ACR_PASSWORD": "your-acr-password-here (or Key Vault reference in Azure)",
    "CONTAINER_IMAGE": "valheimacrXXXX.azurecr.io/valheim-server:latest"
  }
}
